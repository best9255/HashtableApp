<Window x:Class="HashtableApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashtableApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Height="113" Margin="26,217,0,0" VerticalAlignment="Top" Width="221" Click="addBtn_Click"/>
        <Button Content="Remove" HorizontalAlignment="Center" Height="113" Margin="0,217,0,0" VerticalAlignment="Top" Width="220" Click="Button_Click"/>
        <Button Content="Show all" HorizontalAlignment="Left" Height="113" Margin="550,217,0,0" VerticalAlignment="Top" Width="221" Click="Button_Click_1"/>
        <TextBox x:Name="keyData" HorizontalAlignment="Left" Height="56" Margin="26,30,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="745"/>
        <TextBox x:Name="valueData" HorizontalAlignment="Left" Height="56" Margin="26,116,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="745"/>

    </Grid>
</Window>
